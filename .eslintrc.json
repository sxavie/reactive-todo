{
  "extends": ["next", "next/core-web-vitals", "eslint:recommended"],
  "globals": {
    "React": "readonly"
  },
  "rules": {
    "no-unused-vars": [1, { "args": "after-used", "argsIgnorePattern": "^_" }]
  }
}
/*
"off" or 0 - turn the rule off
"warn" or 1 - turn the rule on as warning (doesn't affect exit code)
"error" or 2 - turn the rule on as error (exit code will be 1)
*/
